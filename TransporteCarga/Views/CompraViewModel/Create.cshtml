                    @*Comparación de Precios*@
                    <div class="ibox ">
                        <div class="ibox-content">
                            <div class="tab-content">

                                @foreach (var item in Model.Productos)
                                {

                                    <div id="contact-@Html.DisplayFor(modelItem => item.productoId)" class="tab-pane">
                                        <div class="row m-b-lg">
                                            <div class="col-lg-12">
                                                <h3>
                                                    <strong>
                                                        @Html.DisplayFor(modelItem => item.nombre)
                                                    </strong>
                                                </h3>

                                            </div>
                                        </div>
                                        <div class="client-detail">
                                            <div class="full-height-scroll">
                                                <hr />
                                                <strong>Comparación de Precios</strong>
                                                <div id="vertical-timeline" class="vertical-container dark-timeline">
                                                    <div class="vertical-timeline-block">
                                                        <div class="vertical-timeline-icon gray-bg">
                                                            <i class="fa fa-coffee"></i>
                                                        </div>
                                                        <div class="vertical-timeline-content">
                                                            <p>
                                                                @{
                                    double? maxValue = 0;
                                    string maxProveedor = "";


                                    if (item.Compras.Count > 0)
                                    {
                                        maxValue = item.Compras.Max(p => p.precioUnitario);
                                        maxProveedor = item.Compras.Where(p => p.precioUnitario == maxValue).FirstOrDefault().Compra.Proveedor.razonSocial;

                                    }

                                                                }
                                                                Precio Mayor: @Html.DisplayFor(modelItem => maxValue)
                                                            </p>
                                                            <p>
                                                                Proveedor: @Html.DisplayFor(modelItem => maxProveedor)
                                                            </p>
                                                            @*<span class="vertical-date small text-muted"> 2:10 pm - 12.06.2014 </span>*@
                                                        </div>
                                                    </div>
                                                    <div class="vertical-timeline-block">
                                                        <div class="vertical-timeline-icon gray-bg">
                                                            <i class="fa fa-briefcase"></i>
                                                        </div>
                                                        <div class="vertical-timeline-content">
                                                            <p>
                                                                @{
                                    double? minValue = 0;
                                    string minProveedor = "";


                                    if (item.Compras.Count > 0)
                                    {
                                        minValue = item.Compras.Min(p => p.precioUnitario);
                                        minProveedor = item.Compras.Where(p => p.precioUnitario == minValue).Last().Compra.Proveedor.razonSocial;

                                    }

                                                                }
                                                                Precio Menor: @Html.DisplayFor(modelItem => minValue)
                                                            </p>
                                                            <p>
                                                                Proveedor: @Html.DisplayFor(modelItem => minProveedor)
                                                            </p>
                                                            @*<span class="vertical-date small text-muted"> 4:20 pm - 10.05.2014 </span>*@
                                                        </div>
                                                    </div>
                                                    <div class="vertical-timeline-block">
                                                        <div class="vertical-timeline-icon gray-bg">
                                                            <i class="fa fa-bolt"></i>
                                                        </div>
                                                        <div class="vertical-timeline-content">
                                                            <p>
                                                                @{
                                    double? lastValue = 0;
                                    string lastProveedor = "";


                                    if (item.Compras.Count > 0)
                                    {
                                        lastValue = item.Compras.LastOrDefault().precioUnitario;
                                        lastProveedor = item.Compras.Where(p => p.precioUnitario == lastValue).Last().Compra.Proveedor.razonSocial;

                                    }

                                                                }
                                                                Último Precio: @Html.DisplayFor(modelItem => lastValue)
                                                            </p>
                                                            <p>
